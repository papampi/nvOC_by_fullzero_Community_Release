if [ $COIN == "NICE-EQUIHASH" ]
then
  ADDR="$NICE_ADDRESS$WALLET_ADDRESS_FORMAT$NICE_WORKER"
  if [ $EWBF_VERSION == "3_3" ]
  then
    HCD="${NVOC}/miners/ewbf/v3_3/miner"
  fi
  if [ $EWBF_VERSION == "3_4" ]
  then
    HCD="${NVOC}/miners/ewbf/v3_4/miner"
  fi
  if [ $EQUIHASH_MINER == "ZM" ]
  then
    HCD="${NVOC}/miners/dstm/latest/zm_miner"
    screen -dmSL miner $HCD --server $NICE_EQUIHASH_POOL --user $ADDR --pass $MINER_PWD --port $NICE_EQUIHASH_PORT  --time $ZM_OPTS ;
  fi
  if [ $EQUIHASH_MINER == "EWBF" ]
  then
    screen -dmSL miner $HCD --eexit 3 --fee $EWBF_PERCENT --pec --server $NICE_EQUIHASH_POOL --user $ADDR --pass $MINER_PWD --port $NICE_EQUIHASH_PORT  $EWBF_OPTS ;
  fi
  if [ $EQUIHASH_MINER == "BMINER" ]
  then
    HCD="${NVOC}/miners/bminer/latest/bminer"
    screen -dmSL miner $HCD -uri stratum://$ADDR:$MINER_PWD@$NICE_EQUIHASH_POOL:$NICE_EQUIHASH_PORT $BMINER_OPTS ;
  fi
  if [ $EQUIHASH_MINER == "BMINER_SSL" ]
  then
    HCD="${NVOC}/miners/bminer/latest/bminer"
    screen -dmSL miner $HCD -uri stratum+ssl://$ADDR:$MINER_PWD@$NICE_EQUIHASH_POOL:$NICE_EQUIHASH_PORT $BMINER_OPTS ;
  fi
fi

