# 0miner for nvOC v0019-2.1 by papampi

# DEV Mod Log:
# v=0001 : papampi: Initial Release
#
# v=0002 : papampi:
#          fix lbc
#
# v=0003 : papampi:
#          More NiceHash, MINER_PWD failsafe,
#          COIN_EXTENSION_ARGUMENTS for ethminer and genoil
#
# v=0004 : damNmad:
#          New Coins LUX, SUMO, DSR
# v=0005 : papampi:
#          New Coin ELLA,  XVG, GBX, CRC
#
# v=0006 : papampi:
#          EQUIHASH_MINER, bminer, ETHASH_MINER
# v=0007 : papampi:
#          Nicehash bug fixes
# v=0008 : papampi:
#          Move all miners to ${NVOC}/miners/
# v=0009 : papampi
#          Pool Wallet Address Format
# v=0010 : papampi
#          New Claymore Dual Mining
# v=0011 : damNmad
#          Added BTCP 				  

nvOC_Ver="nvOC v0019-2.1 - Community Release"
nvOC_0miner_ver="v0019-2.0.0011"   # Do not edit this

source ${NVOC}/1bash

# Set MINER_PWD if unset
if [ -z ${MINER_PWD+x} ]
then
  MINER_PWD="x"
fi

if [ $COIN == "CLAYMOIRE_DUAL" ]
then
  HCD="${NVOC}/miners/claymore/$CLAYMORE_VERSION/ethdcrminer64"
  E_ADDR="$DUAL_EWAL$WALLET_ADDRESS_FORMAT$DUAL_EWORKER"
  D_ADDR="$DUAL_DWAL$WALLET_ADDRESS_FORMAT$DUAL_DWORKER"
  screen -dmSL miner $HCD -epool $DUAL_EPOOL:$DUAL_EPORT -ewal $E_ADDR -epsw  $DUAL_EPSW -dpool stratum+tcp://$DUAL_DPOOL:$DUAL_DPORT -dwal $D_ADDR -dpsw $DUAL_DPSW -dcoin $DUAL_DCOIN -allpools $ALL_POOLS -dbg -1 -mport $MPORT -mpsw $MPSW $DUAL_ADDITIONAL_ARGUMENTS
fi	

if [ $COIN == "ONION" ]
then
  HCD="${NVOC}/miners/TPccminer/ccminer"
  ADDR="$ONION_ADDRESS"
  screen -dmSL miner $HCD -a x13 -o stratum+tcp://$ONION_POOL:$ONION_PORT -u $ADDR -p c=ONION
fi

if [ $COIN == "SMART" ]
then
  HCD="${NVOC}/miners/ASccminer/ccminer"
  ADDR="$SMART_ADDRESS$WALLET_ADDRESS_FORMAT$SMART_WORKER"
  screen -dmSL miner $HCD -a keccak -o stratum+tcp://$SMART_POOL:$SMART_PORT -u $ADDR -p $MINER_PWD -i $SMART_INTENSITY
fi

if [ $COIN == "BTG" ]
then
  BTGADDR="$BTG_ADDRESS$WALLET_ADDRESS_FORMAT$BTG_WORKER"
  if [ $EWBF_VERSION == "3_3" ]
  then
    HCD="${NVOC}/miners/ewbf/v3_3/miner"
  fi
  if [ $EWBF_VERSION == "3_4" ]
  then
    HCD="${NVOC}/miners/ewbf/v3_4/miner"
  fi
  if [ $EQUIHASH_MINER == "ZM" ]
  then
    HCD="${NVOC}/miners/dstm/latest/zm_miner"
    screen -dmSL miner $HCD --server $BTG_POOL --user $BTGADDR --pass $MINER_PWD --port $BTG_PORT  --time $ZM_OPTS ;
  fi
  if [ $EQUIHASH_MINER == "EWBF" ]
  then
    screen -dmSL miner $HCD --eexit 3 --fee $EWBF_PERCENT --pec --server $BTG_POOL --user $BTGADDR --pass $MINER_PWD --port $BTG_PORT  $EWBF_OPTS ;
  fi
  if [ $EQUIHASH_MINER == "BMINER" ]
  then
    HCD="${NVOC}/miners/bminer/latest/bminer"
    screen -dmSL miner $HCD -uri stratum://$BTGADDR:$MINER_PWD@$BTG_POOL:$BTG_PORT $BMINER_OPTS ;
  fi
  if [ $EQUIHASH_MINER == "BMINER_SSL" ]
  then
    HCD="${NVOC}/miners/bminer/latest/bminer"
    screen -dmSL miner $HCD -uri stratum+ssl://$BTGADDR:$MINER_PWD@$BTG_POOL:$BTG_PORT $BMINER_OPTS ;
  fi
fi

if [ $COIN == "ZEC" ]
then
  ZECADDR="$ZEC_ADDRESS$WALLET_ADDRESS_FORMAT$ZEC_WORKER"
  if [ $EWBF_VERSION == "3_3" ]
  then
